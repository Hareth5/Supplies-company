<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/17.0.13"
	xmlns:fx="http://javafx.com/fxml/1" prefHeight="900.0"
	prefWidth="1566.0">

	<center>
		<TabPane fx:id="tabPane" prefHeight="800.0" prefWidth="1400.0">
			<tabs>
				<Tab text="ProductsAndShipments">
					<TextArea fx:id="productsShipmentsArea" wrapText="true"
						editable="false" prefHeight="800.0" prefWidth="1400.0" />
				</Tab>
				<Tab text="OrdersAndCustomers">
					<TextArea fx:id="ordersCustomersArea" wrapText="true"
						editable="false" prefHeight="800.0" prefWidth="1400.0" />
				</Tab>
			</tabs>
		</TabPane>
	</center>
</BorderPane>
